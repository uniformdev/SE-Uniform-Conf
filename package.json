		"@uniformdev/canvas-kontent": "19.10.0",
		"@uniformdev/canvas-next": "19.10.0",
		"@uniformdev/canvas-react": "19.10.0",
		"@uniformdev/canvas-sanity": "19.10.0",
		"@uniformdev/canvas-strapi": "^17.1.0",
		"@uniformdev/context": "19.10.0",
		"@uniformdev/context-devtools": "19.10.0",
		"@uniformdev/context-gtag": "19.10.0",
		"@uniformdev/context-next": "19.10.0",
		"@uniformdev/context-react": "19.10.0",
		"@uniformdev/project-map": "19.10.0",
		"analytics": "0.8.1",
		"contentful": "^10.2.1",
		"contentstack": "^3.17.1",
		"cookie": "0.5.0",
		"graphql-request": "^6.0.0",
		"next": "13.4.3",
