		"@types/cookie": "0.5.1",
		"@types/node": "20.2.3",
		"@types/react": "18.2.6",
		"@types/react-dom": "18.2.4",
		"@types/styled-components": "5.1.26",
		"@uniformdev/cli": "^19.10.0",
		"autoprefixer": "^10.4.14",
		"npm-run-all": "4.1.5",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"sass": "1.62.1",
		"tailwindcss": "3.3.2",
		"ts-loader": "9.4.2",
		"typescript": "5.0.4"
	}
}