{
	"name": "se-uniform-conf",
	"version": "19.6.0",
	"private": true,
	"license": "Apache-2.0",
	"scripts": {
		"start": "next start -p 3210",
		"dev": "run-s context:manifest dev:next",
		"dev:next": "next dev -p 3210",
		"build": "run-s context:manifest build:next",
		"build:app": "run-s context:manifest build:next",
		"build:next": "next build",
		"context:manifest": "uniform context manifest download --output ./src/lib/context/manifest.json",
		"pull": "run-s pull:canvas pull:project-map pull:context",
		"pull:canvas": "run-s pull:components pull:compositions",
		"pull:project-map": "run-s pull:project-map-definition pull:project-map-nodes",
		"pull:context": "run-s pull:context:signals pull:context:enrichments pull:context:quirks pull:context:tests pull:context:aggregates",
		"pull:compositions": "uniform canvas composition pull ./data/compositions",
		"pull:components": "uniform canvas component pull ./data/components",
		"pull:context:signals": "uniform context signal pull ./data/context/signals",
		"pull:context:enrichments": "uniform context enrichment pull ./data/context/enrichments",
		"pull:context:aggregates": "uniform context aggregate pull ./data/context/aggregates",
		"pull:context:quirks": "uniform context quirk pull ./data/context/quirks",
		"pull:context:tests": "uniform context test pull ./data/context/tests",
		"pull:project-map-definition": "uniform project-map definition pull ./data/project-map/definition",
		"pull:project-map-nodes": "uniform project-map node pull ./data/project-map/nodes",
		"push": "run-s push:canvas push:project-map push:context",
		"push:canvas": "run-s push:canvas:components push:canvas:compositions push:canvas:publish_compositions",
		"push:project-map": "run-s push:project-map-definition push:project-map-nodes",
		"push:context": "run-s push:context:signals push:context:enrichments push:context:publish-manifest",
		"push:canvas:components": "uniform canvas component push ./data/components",
		"push:canvas:compositions": "uniform canvas composition push ./data/compositions",
		"push:canvas:publish_compositions": "uniform canvas composition publish --all",
		"push:project-map-definition": "uniform project-map definition push ./data/project-map/definition",
		"push:project-map-nodes": "uniform project-map node push ./data/project-map/nodes",
		"push:context:signals": "uniform context signal push ./data/context/signals",
		"push:context:enrichments": "uniform context enrichment push ./data/context/enrichments",
		"push:context:aggregates": "uniform context aggregate push ./data/context/aggregates",
		"push:context:quirks": "uniform context quirk push ./data/context/quirks",
		"push:context:tests": "uniform context test push ./data/context/tests",
		"push:context:publish-manifest": "uniform context manifest publish",
		"package": "run-s package:canvas package:context",
		"package:canvas": "run-s package:components package:compositions",
		"package:compositions": "uniform canvas composition pull ./data/uniformconf.yaml",
		"package:components": "uniform canvas component pull ./data/uniformconf.yaml",
		"package:context:signals": "uniform context signal pull ./data/uniformconf.yaml",
		"package:context:enrichments": "uniform context enrichment pull ./data/uniformconf.yaml",
		"package:context:aggregates": "uniform context aggregate pull ./data/uniformconf.yaml",
		"package:context:quirks": "uniform context quirk pull ./data/uniformconf.yaml",
		"package:context:tests": "uniform context test pull ./data/uniformconf.yaml",
		"package:context": "run-s package:context:signals package:context:enrichments package:context:quirks package:context:tests package:context:aggregates",
		"build:preview": "tsup"
	},
	"dependencies": {
		"@analytics/google-analytics": "1.0.5",
